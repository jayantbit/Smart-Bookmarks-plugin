{
  "name": "Smart Bookmarks",
  "version": "2.0",
  "description": "::Search your bookmarks faster & get suggested bookmarks according to page URL:: By Jayant Mukherji",
  
  "permissions": [
    "bookmarks","tabs"
  ],
  "browser_action": {
      "default_title": "Smart Bookmarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com  https://ssl.google-analytics.com;  object-src 'self'"
}
